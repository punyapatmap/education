@article{Sur2013,
  title = {Impact of Statins on Nephrolithiasis in Hyperlipidemic Patients: A 10-Year Review of  an Equal Access Health Care System.},
  author = {Sur, Roger L and Masterson, James H and Palazzi, Kerrin L and L'Esperance, James O and Auge, Brian K and Chang, David C and Stoller, Marshall L},
  date = {2013-05},
  journaltitle = {Clinical nephrology},
