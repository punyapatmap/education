@article{Ding2019,
  title = {Association between {{Dyslipidemia}} and {{Nephrolithiasis Risk}} in a {{Chinese Population}}.},
  author = {Ding, Qi and Ouyang, Jun and Fan, Bo and Cao, Cheng and Fan, Zhijiang and Ding, Li and Li, Feng and Tu, Wenjian and Jin, Xiaohua and Wang, Jing and Shi, Ying},
  date = {2019},
  journaltitle = {Urologia internationalis},
  volume = {103},
