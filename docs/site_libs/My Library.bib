@article{Carney2023,
  title = {Using Learning Analytics in Clinical Competency Committees: {{Increasing}} the Impact of Competency-Based Medical Education},
  author = {Carney, Patricia A. and Sebok-Syer, Stefanie S. and Pusic, Martin V. and Gillespie, Colleen C. and Westervelt, Marjorie and Goldhamer, Mary Ellen J.},
  date = {2023},
  journaltitle = {Medical Education Online},
  volume = {28},
