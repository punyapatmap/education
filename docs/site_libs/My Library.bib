@article{DeJong2018,
  title = {The Role of the E-Tutor in Synchronous Online Problem-Based Learning: {{A}} Study in a {{Master Public Health Programme}}},
  author = {family=Jong, given=Nynke, prefix=de, useprefix=true and Verstegen, Daniëlle M.L. and Könings, Karen D.},
