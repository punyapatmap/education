@article{Watari2020,
  title = {Factors and Impact of Physicians' Diagnostic Errors in Malpractice Claims in {{Japan}}},
  author = {Watari, Takashi and Tokuda, Yasuharu and Mitsuhashi, Shohei and Otuki, Kazuya and Kono, Kaori and Nagai, Nobuhiro and Onigata, Kazumichi and Kanda, Hideyuki},
  date = {2020},
  journaltitle = {PLoS ONE},
