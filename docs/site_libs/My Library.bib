@article{Mains2015,
  title = {Do Questions Help? {{The}} Impact of Audience Response Systems on Medical Student Learning: {{A}} Randomized Controlled Trial},
  author = {Mains, Tyler E. and Cofrancesco, Joseph and Milner, Stephen M. and Shah, Nina G. and Goldberg, Harry},
