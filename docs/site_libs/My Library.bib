@article{Schauber2020,
  title = {How Sure Can We Be That a Student Really Failed? {{On}} the Measurement Precision of Individual Pass-Fail Decisions from the Perspective of {{Item Response Theory}}},
  author = {Schauber, Stefan K. and Hecht, Martin},
  date = {2020},
  journaltitle = {Medical Teacher},
  volume = {42},
  number = {12},
