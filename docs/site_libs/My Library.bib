@article{Kiesewetter2013,
  title = {Cognitive {{Problem Solving Patterns}} of {{Medical Students Correlate}} with {{Success}} in {{Diagnostic Case Solutions}}},
  author = {Kiesewetter, Jan and Ebersbach, René and Görlitz, Anja and Holzer, Matthias and Fischer, Martin R. and Schmidmaier, Ralf},
  date = {2013},
  journaltitle = {PLoS ONE},
