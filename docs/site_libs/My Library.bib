  author = {Wang, Anan and Xiao, Ruihan and Zhang, Chun and Yuan, Lin and Lin, Nana and Yan, Lu and Wang, Yaohua and Yu, Jinhai and Huang, Qin and Gan, Puying and Xiong, Chao and Xu, Qihua and Liao, Hongfei},
  date = {2022-12-01},
  journaltitle = {BMC Medical Education},
