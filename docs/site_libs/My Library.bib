}

@article{Wolcott2021,
  title = {A Review to Characterise and Map the Growth Mindset Theory in Health Professions Education},
  author = {Wolcott, Michael D. and McLaughlin, Jacqueline E. and Hann, Alice and Miklavec, Amelia and Beck Dallaghan, Gary L. and Rhoney, Denise H. and Zomorodi, Meg},
  date = {2021},
  journaltitle = {Medical Education},
