  author = {Findyartini, Ardi and Syah, Nur Afrainin and Susilo, Astrid Pratidina and Nurokhmanti, Hikmawati and Qomariyah, Nurul and Greviana, Nadia and Ainin, Dina Qurratu and Sari, Sylvia Mustika and Claramita, Mora},
