@article{garza.etal_2023,
  title = {Performance in {{Kahoot}}! Activities as Predictive of Exam Performance},
  author = {Garza, Mc and Olivan, S and Monleón, E and Cisneros, Ana Isabel and García-Barrios, A and Ochoa, I and Whyte, J and Lamiquiz-Moneo, I},
  date = {2023-06-06},
  journaltitle = {BMC Medical Education},
  shortjournal = {BMC Med Educ},
  volume = {23},
  number = {1},
  pages = {413},
  issn = {1472-6920},
  doi = {10.1186/s12909-023-04379-x},
  url = {https://bmcmededuc.biomedcentral.com/articles/10.1186/s12909-023-04379-x},
  urldate = {2023-11-12},
  abstract = {Background\enspace{} Game-based learning (GBL) is effective for increasing participation, creativity, and student motivation. However, the discriminative value of GBL for knowledge acquisition has not yet been proven. The aim of this study is to assess the value of Kahoot! as a discriminative tool for formative assessment in medical education in two different subjects. Methods\enspace{} A prospective experimental study was conducted on a sample of 173 students enrolled in neuroanatomy (2021–2022). One hundred twenty-five students individually completed the Kahoot! prior to the final exam. In addition, students enrolled in human histology during two academic courses were included in the study. The control group course (2018–2019) received a traditional teaching methodology (N\,=\,211), while Kahoot! was implemented during 2020–2021 (N\,=\,200). All students completed similar final exams for neuroanatomy and human histology based on theory tests and image exams. Results\enspace{} The correlation between the Kahoot score and the final grade was analyzed for all students enrolled in neuroanatomy who completed both exercises. The correlation between the Kahoot exercise and the theory test, image exam and final grade was significantly positive in all cases (r\,=\,0.334 p\,{$<$}\,0.001, r\,=\,0.278 p\,=\,0.002 and r\,=\,0.355 p\,{$<$}\,0.001, respectively). Moreover, students who completed the Kahoot! exercise obtained significantly higher grades in all parts of the exam. Regarding human histology, the theory tests, image exams and final grades were significantly higher when using Kahoot! versus the “traditional” methodology (p\,{$<$}\,0.001, p\,{$<$}\,0.001 and p\,=\,0.014, respectively). Conclusions\enspace{} Our study demonstrates for the first time that Kahoot! can be used to improve and predict the final grade in medical education subjects.},
  langid = {english},
  file = {/Users/prabprime/Zotero/storage/C8XG2JFR/Garza et al. - 2023 - Performance in Kahoot! activities as predictive of.pdf}
}

