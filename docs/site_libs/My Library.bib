  author = {Aung, Myo Nyein and Wannakrairot, Pongsak and Somboonwong, Juraiporn and Jaroonvanichkul, Vorapol},
  date = {2015-12},
  journaltitle = {International Journal of General Medicine},
  shortjournal = {IJGM},
