  author = {Thompson, Britta M. and Haidet, Paul and Borges, Nicole J. and Carchedi, Lisa R. and Roman, Brenda J.B. and Townsend, Mark H. and Butler, Agata P. and Swanson, David B. and Anderson, Michael P. and Levine, Ruth E.},
  date = {2015},
