@article{2021_charan.etal,
  title = {Sample {{Size Calculation}} in {{Medical Research}}: {{A Primer}}},
  shorttitle = {Sample {{Size Calculation}} in {{Medical Research}}},
  author = {Charan, Jaykaran and Kaur, Rimplejeet and Bhardwaj, Pankaj and Singh, Kuldeep and Ambwani, Sneha R. and Misra, Sanjeev},
  date = {2021-04},
  journaltitle = {Annals of the National Academy of Medical Sciences (India)},
  shortjournal = {Annals of the National Academy of Medical Sciences (India)},
  volume = {57},
