@article{joksimovic.etal_2015a,
  title = {Learning at Distance: {{Effects}} of Interaction Traces on Academic Achievement},
  shorttitle = {Learning at Distance},
  author = {Joksimović, Srećko and Gašević, Dragan and Loughin, Thomas M. and Kovanović, Vitomir and Hatala, Marek},
