  author = {family=Gaalen, given=A. E.J., prefix=van, useprefix=true and Brouwer, J. and Schönrock-Adema, J. and Bouwkamp-Timmer, T. and Jaarsma, A. D.C. and Georgiadis, J. R.},
  date = {2021},
  journaltitle = {Advances in Health Sciences Education},
  volume = {26},
