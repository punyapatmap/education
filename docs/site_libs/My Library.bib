@article{Purnajati2021,
  title = {Does Objective Structured Clinical Examination Examiners’ Backgrounds Influence the Score Agreement?},
  author = {Purnajati, Oscar Gilang and Hidayah, Rachmadya Nur and Rahayu, Gandes Retno},
