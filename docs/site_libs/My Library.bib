@article{Yoo2021,
  title = {Adaptations in {{Anatomy Education}} during {{COVID-19}}},
  author = {Yoo, Hyeijung and Kim, Dasom and Lee, Young Mee and Rhyu, Im Joo},
  date = {2021},
  journaltitle = {Journal of Korean Medical Science},
  volume = {36},
  number = {1},
