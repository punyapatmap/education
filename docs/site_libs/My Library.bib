@article{Singh2013,
  title = {Qualities of an Effective Teacher: {{What}} Do Medical Teachers Think?},
  author = {Singh, Simerjit and Pai, Dinker R. and Sinha, Nirmal K. and Kaur, Avneet and Kyaw Soe, Htoo Htoo and Barua, Ankur},
