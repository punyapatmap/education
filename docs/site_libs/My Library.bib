@article{2023_fischer.etal,
  title = {Using Cognitive Load Theory to Evaluate and Improve Preparatory Materials and Study Time for the Flipped Classroom},
  author = {Fischer, Krisztina and Sullivan, Amy M. and Cohen, Amy P. and King, Randall W. and Cockrill, Barbara A. and Besche, Henrike C.},
  date = {2023-05-17},
  journaltitle = {BMC Medical Education},
  shortjournal = {BMC Med Educ},
  volume = {23},
  number = {1},
  pages = {345},
  issn = {1472-6920},
  doi = {10.1186/s12909-023-04325-x},
  url = {https://bmcmededuc.biomedcentral.com/articles/10.1186/s12909-023-04325-x},
  urldate = {2023-11-02},
  abstract = {Background\enspace{} Preclinical medical education is content-dense and time-constrained. Flipped classroom approaches promote durable learning, but challenges with unsatisfactory student preparation and high workload remain. Cognitive load theory defines instructional design as “efficient” if learners can master the presented concepts without cognitive overload. We created a PReparatory Evaluation Process (PREP) to systematically assess and measure improvement in the cognitive-load efficiency of preparatory materials and impact on study time (time-efficiency). Methods\enspace{} We conducted this study in a flipped, multidisciplinary course for \textasciitilde\,170 first year students at Harvard Medical School using a naturalistic post-test design. For each flipped session (n\,=\,97), we assessed cognitive load and preparatory study time by administering a 3-item PREP survey embedded within a short subject-matter quiz students completed before class. Over three years (2017–2019), we evaluated cognitive load- and time- based efficiency to guide iterative revisions of the materials by content experts. The ability of PREP to detect changes to the instructional design (sensitivity) was validated through a manual audit of the materials. Results\enspace{} The average survey response rate was ≥ 94\%. Content expertise was not required to interpret PREP data. Initially students did not necessarily allocate the most study time to the most difficult content. Over time, the iterative changes in instructional design increased the cognitive load- and time-based efficiency of preparatory materials with large effect sizes (p\,{$<$}\,.01). Furthermore, this increased the overall alignment of cognitive load with study time: students allocated more time to difficult content away from more familiar, less difficult content without increasing workload overall. Conclusions\enspace{} Cognitive load and time constraints are important parameters to consider when designing curricula. The PREP process is learner-centered, grounded in educational theory, and works independently of content knowledge. It can provide rich and actionable insights into instructional design of flipped classes not captured by traditional satisfaction-based evaluations.},
  langid = {english},
  file = {/Users/prabprime/Documents/Work/Resources/All/MMSc-Harvard/QRIME/Mini Project - LMS/2023_BMC Medical Education_Fischer_Using cognitive load theory to evaluate and improve preparatory materials and.pdf}
}

