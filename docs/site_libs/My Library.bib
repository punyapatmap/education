@article{lu.etal_2021,
  title = {Impact of Teachers’ Grading Policy on the Identification of at-Risk Students in Learning Analytics},
  author = {Lu, Owen H.T. and Huang, Anna Y.Q. and Yang, Stephen J.H.},
  date = {2021-04},
  journaltitle = {Computers \& Education},
