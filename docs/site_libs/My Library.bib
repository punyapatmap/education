  author = {McGowan, Jessie and Sampson, Margaret and Salzwedel, Douglas M. and Cogo, Elise and Foerster, Vicki and Lefebvre, Carol},
  date = {2016-07},
  journaltitle = {Journal of Clinical Epidemiology},
