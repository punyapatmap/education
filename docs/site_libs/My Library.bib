  shorttitle = {Robust, Defensible, and Fair},
  author = {Cleland, J. and Blitz, J. and Cleutjens, K. B. J. M. and Oude Egbrink, M. G. A. and Schreurs, S. and Patterson, F.},
  date = {2023-10},
